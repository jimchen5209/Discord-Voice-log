{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": [
            false
        ],
        "quotemark": [
            true,
            "single"
        ],
        "curly": [
            true,
            "ignore-same-line"
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "max-line-length": [
            true,
            210
        ],
        "trailing-comma": false,
        "no-console": false
    },
    "linterOptions": {
        "exclude": [
            "Libs/*/*.js",
            "ecosystem.config.js"
        ]
    },
    "rulesDirectory": []
}